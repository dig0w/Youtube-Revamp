{
    "name": "Youtube Improved",
    "description": "This extension imrpoves the Youtube experience. Features: Ad Blocker (Not only on Youtube) | Dislikes | Download Videos (Beta)",
    "version": "0.3",
    "manifest_version": 3,
    "background": {
        "service_worker": "background.js"
    },
    "content_scripts": [
        {
          "matches": ["<all_urls>"],
          "js": ["script.js"]
        }
    ],
    "permissions": ["webRequest","declarativeNetRequest","tabs","activeTab","declarativeContent","downloads"],
    "icons": {
        "16": "icons/logo_16x.png",
        "32": "icons/logo_32x.png",
        "48": "icons/logo_48x.png",
        "128": "icons/logo_128x.png"
    }
}